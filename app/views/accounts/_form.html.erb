<%= simple_form_for(@account) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :owner_email %>
    <%= f.input :name %>
    <%= f.input :account_type, collection: ['Restaurant', 'Takeaway'] %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
